# Base domain entries - converting AD-style DN to OpenLDAP format
dn: dc=mock,dc=example,dc=org
objectClass: top
objectClass: dcObject
objectClass: organization
dc: mock
o: Mock Organization

# Organizational Units
dn: ou=Users,dc=mock,dc=example,dc=org
objectClass: top
objectClass: organizationalUnit
ou: Users

dn: ou=Groups,dc=mock,dc=example,dc=org
objectClass: top
objectClass: organizationalUnit
ou: Groups

# Admin group
dn: cn=admins,ou=Groups,dc=mock,dc=example,dc=org
objectClass: top
objectClass: groupOfNames
cn: admins
description: An administrators group

# group-mock-1 group
